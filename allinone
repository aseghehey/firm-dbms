create table HasBought (
	ClientID INT NOT NULL REFERENCES Clients,
	InvestmentID INT NOT NULL REFERENCES Investment,
	Price DECIMAL(7,2),
	DateBought DATE,CONSTRAINT hasbought_pk PRIMARY KEY(ClientID,InvestmentID)
);

