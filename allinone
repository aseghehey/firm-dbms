create table Brokers (
	EID INT PRIMARY KEY,
	first_name VARCHAR(50),
	last_name VARCHAR(50),
	Password VARCHAR(50),
	StartDate DATE,
	Salary DECIMAL(7,2),
	Branch INT NOT NULL REFERENCES Branches
);
