CREATE VIEW CustomerView(FirstName, LastName, InitialAmount, CurrentAmount, Address, BrokerName) AS SELECT FirstName, LastName, InitialAmount, CurrentAmount, Address, last_name FROM Clients C, Brokers B WHERE C.Broker = B.EID;
CREATE VIEW TopBrokers(BID, FirstName,LastName, BranchID, TotalEarnings) AS SELECT B.EID, B.first_name, B.last_name, Branch, SUM(CurrentAmount) FROM Brokers B, Clients C WHERE B.EID = C.Broker GROUP BY EID;